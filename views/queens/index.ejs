<%- include('../partials/html-head') %>
<link rel="stylesheet" href="/stylesheets/queens/index.css" />
<%- include('../partials/nav') %>

<main>
  <h1>MY GIRLS</h1>
  <section id="queen-card">
    <% queens.forEach(q => { %>
    <div <%- q.eliminated ? 'class="eliminated"' : 'class=""' %>
      <h3>image here</h3>
      <p><%= q.name %></p>
      <p><%= q.hometown %></p>
      <p><%= q.pointEvents.reduce((prev, event) => {
        prev.points + event.points
      }, 0) %>
      </p>
      <p>Eliminated? <input type="checkbox" name="eliminated"></p>
    </div>

    <% }) %>
  </section>
</main>

<%- include('../partials/footer') %>
